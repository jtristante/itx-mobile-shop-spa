{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "endOfLine": "auto",
  "printWidth": 120,
  "importOrder": [
    "^(react|react-dom|react-router)(/.*)?$",
    "<THIRD_PARTY_MODULES>",
    "^(assets|components|hooks|locales|mocks|model|pages|services|styles|theme|utils)/(.*)|^(store|App|i18n)$",
    "^(..|.)/(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
